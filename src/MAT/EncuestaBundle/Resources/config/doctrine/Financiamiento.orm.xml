<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MAT\EncuestaBundle\Entity\Financiamiento" table="financiamiento">
    <indexes>
      <index name="IDX_CCF9C6FB4ED703C9" columns="id_finca"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="bancoInstitucion" type="boolean" column="banco_institucion" nullable="true"/>
    <field name="requeVacas" type="boolean" column="reque_vacas" nullable="true"/>
    <field name="requeNovillas" type="boolean" column="reque_novillas" nullable="true"/>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <many-to-one field="idFinca" target-entity="Carne">
      <join-columns>
        <join-column name="id_finca" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
