{% extends 'base.html.twig' %}

{% block body %}
    <h1>Otras Especies</h1>

    <table>
        <thead>
            <tr>
                <th>Numbufalinosbutoros</th>
                <th>Numbufalinosbufalas</th>
                <th>Numbufalinosbubillos</th>
                <th>Numbufalinosbumautes</th>
                <th>Numbufalinosbucerros</th>
                <th>Numcaprinoscabrio</th>
                <th>Numcaprinoscabra</th>
