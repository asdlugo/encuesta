{% extends 'base.html.twig' %}

{% block body %}
    <h1>Nivel de Educación</h1>

    <table>
        <tbody>
            <tr>
                <th>Descripción</th>
                <td>{{ nivelEducacion.descripcion }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ nivelEducacion.id }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('niveleducacion_index') }}">Volver a la lista</a>
        </li>
        <li>
            <a href="{{ path('niveleducacion_edit', { 'id': nivelEducacion.id }) }}">Editar</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Eliminar">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
